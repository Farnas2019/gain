

    <%- include('./partials/header')%>
   
  
    <div class="vizew-login-area section-padding-80">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-12 col-md-6">
                    <div class="login-content">
                        <!-- Section Title -->
                        <div class="section-heading">
                            <h4>Great to have you back!</h4>
                            <div class="line"></div>
                        </div>

                        <form action="/post" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <input type="text" class="form-control" id="exampleInputEmail1" name="postT" placeholder="Tittle">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" id="exampleInputEmail1" name="postC" placeholder="Category">
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" id="exampleInputEmail1" name="spec" placeholder="Specialities">
                            </div>
                            <div class="form-group">
                                <input type="textarea" class="form-control" id="exampleInputEmail1" name="discription" placeholder="Product Discription">
                            </div>
                            <div class="form-group">
                                <label for="fimage">Brand Image Or Company Logo *</label>
                                <input type="file" class="form-control" id="exampleInputEmail1" name="fimage" >
                            </div>
                           
                            <div class="form-group">
                                <label for="fimage">Upload Multiple Product Or company Image *</label>
                                <input type="file" class="form-control" id="exampleInputEmail1" name="image" multiple="multiple" placeholder="">
                            </div>
                            <div class="form-group">
                                <label for="fimage">Upload A Short Video Of the Product Or Company *</label>
                                <input type="file" class="form-control" id="exampleInputEmail1" name="vid" placeholder="">
                            </div>
                            
                            
                            <button type="submit" class="btn vizew-btn w-100 mt-30">POST</button>
                        </form>
                       
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ##### Login Area End ##### -->

    
